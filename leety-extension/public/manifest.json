{
    "manifest_version": 3,
    "name": "Leety",
    "version": "1.0",
    "description": "An AI coding tutor that guides you through LeetCode problems with hints, explanations, and debugging help.",
    "host_permissions": ["https://leetcode.com/*"],
    "permissions": ["activeTab", "scripting", "sidePanel", "tabs", "storage"],

    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": ["https://leetcode.com/problems/*"],
            "js": ["content.js"]
        }
    ],
    "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
    }

}